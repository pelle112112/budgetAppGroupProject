 function PostData(expense){
        const options = {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify(expense),
        };
        fetch(`${APIURL}/`, options)
        .then((res) => res.json())
        .then((data) => console.log(data));
    }

    export default PostData;
